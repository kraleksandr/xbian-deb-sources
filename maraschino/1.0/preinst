		mkdir -p "/usr/local/share/maraschino/.git/objects";
	fi


	if [ ! -d "/usr/local/share/maraschino/.git/objects/pack" ]; then
		mkdir -p "/usr/local/share/maraschino/.git/objects/pack";
	fi


	if [ ! -d "/usr/local/share/maraschino/.git/objects/info" ]; then
		mkdir -p "/usr/local/share/maraschino/.git/objects/info";
	fi


	if [ ! -d "/usr/local/share/maraschino/modules" ]; then
		mkdir -p "/usr/local/share/maraschino/modules";
	fi


	if [ ! -d "/usr/local/share/maraschino/maraschino" ]; then
		mkdir -p "/usr/local/share/maraschino/maraschino";
	fi


	if [ ! -d "/usr/local/share/maraschino/static" ]; then
		mkdir -p "/usr/local/share/maraschino/static";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/mobile" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/mobile";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/player_buttons" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/player_buttons";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/star_ratings" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/star_ratings";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/placeholder" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/placeholder";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/weather" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/weather";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/backgrounds" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/backgrounds";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/applications" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/applications";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/trakt" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/trakt";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/notifications" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/notifications";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/images/alpha" ]; then
		mkdir -p "/usr/local/share/maraschino/static/images/alpha";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/js" ]; then
		mkdir -p "/usr/local/share/maraschino/static/js";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/js/lib" ]; then
		mkdir -p "/usr/local/share/maraschino/static/js/lib";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/js/mobile" ]; then
		mkdir -p "/usr/local/share/maraschino/static/js/mobile";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/less" ]; then
		mkdir -p "/usr/local/share/maraschino/static/less";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/less/mobile" ]; then
		mkdir -p "/usr/local/share/maraschino/static/less/mobile";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/less/mobile/lib" ]; then
		mkdir -p "/usr/local/share/maraschino/static/less/mobile/lib";
	fi


	if [ ! -d "/usr/local/share/maraschino/static/less/mobile/lib/images" ]; then
		mkdir -p "/usr/local/share/maraschino/static/less/mobile/lib/images";
	fi

	if [ ! -d "/etc/default" ]; then
		mkdir -p "/etc/default";
	fi
fi
